{
	"Blog Link": {
		"prefix": "lb",
		"body": [
			"> [参考博客: $1]($2)"
		],
		"description": "md 的链接"
	}
	,"MD Link": {
		"prefix": "l",
		"body": ["[$1]($2)"],
		"description": "md 的链接"
	}
	,"MD Table2": {
		"prefix": "ta2",
		"body": [
			"| $1 | $2 |",
			"|:----:|:----:|",
			"| $3 | $4 |"
		],
		"description": "2X2表格"
	}
	,"MD Table3": {
		"prefix": "ta3",
		"body": [
			"| $1 | $2 | $3 |",
			"|:----:|:----:|:----:|",
			"| $4 | $5 | $6 |"
		],
		"description": "3X2表格"
	}
	,"MD Table4": {
		"prefix": "ta4",
		"body": [
			"| $1 | $2 | $3 | $4 |",
			"|:----:|:----:|:----:|:----:|",
			"| $5 | $6 | $7 | $8 |"
		],
		"description": "4X2表格"
	}
	,"Img link": {
		"prefix": "img",
		"body": [
			"![$1]($2)"
		],
		"description": "图片链接"
	}
	,"TODO": {
		"prefix": "td",
		"body": [
			"- [ ] $1"
		],
		"description": "TODO"
	}
}