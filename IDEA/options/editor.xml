  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern icon="important" case-sensitive="true" pattern="\bNOW\b.*" />
    <filter name="now">
      <pattern index="2" />
    </filter>
  </component>
</application>